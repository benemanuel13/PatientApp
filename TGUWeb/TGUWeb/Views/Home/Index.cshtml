@using TGUWeb.Models.WebApiModels;
@model ServiceUser

@{
    ViewBag.Title = "Home Page";
}

<h1>Prototype Patient App Admin</h1>

<div class="row">
    <div class="col-md-4">
        Name:
    </div>
    <div class="col-md-4">
        @Model.Name
    </div>
</div>

<div class="row">
    <div class="col-md-4">
        NHS Number:
    </div>
    <div class="col-md-4">
        @Model.NHSNumber
    </div>
</div>

<div class="row">
    <div class="col-md-4">
        Under FOLS?
    </div>
    <div class="col-md-4">
        @if (Model.UnderFOLS)
        {
            @Html.Raw("Yes")
        }
        else
        {
            @Html.Raw("No")
        }
    </div>
</div>
<br />
<a href="Home/CarePlan">Click here to view and edit Care Plan</a><br />
<a style="cursor: pointer;" onclick="window.location.href = 'http://tguapp.benemanuel.net/Home/Download'">Click here to download Care Plan</a><br /><br />
